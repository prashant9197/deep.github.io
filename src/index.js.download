                function setOpacity() {
                    if (document.body.scrollTop > 500 || document.documentElement.scrollTop > 500) {
                        logo.style.opacity=1;
                        logoMobile.style.opacity=1;
                    } else {
                        logo.style.opacity=0;
                        logoMobile.style.opacity =0;
